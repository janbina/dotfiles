* {
    background-color: #282a36;
    input-text:       #bbc5ff;
    normal-text:      #bbc5ff;
    selected-bg:      #354067;
    selected-text:    #bbc5ff;               
}
window {
    padding: 10;
}
element {
    padding: 10;
    text-color: var(normal-text);
}
element selected.normal {
    background-color: var(selected-bg);
    text-color: var(selected-text);
}
inputbar {
    padding: 15 10 15 10;
    children: [entry];
}
entry {
    text-color: var(input-text);
}
